{
    "name": "seoservice2020/laravel-timezone",
    "license": "MIT",
    "description": "Timezone storage and retrieval for Laravel",
    "keywords": [
        "laravel",
        "timezone"
    ],
    "authors": [{
            "name": "James Mills",
            "email": "james@clicksco.com"
        },
        {
            "name": "Sergey Pronin",
            "email": "sergotail@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": "5.6.* || 5.7.* || 5.8.* || ^6 || ^7 || ^8",
        "torann/geoip": "^3.0-stable",
        "nesbot/carbon": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "SEOService2020\\LaravelTimezone\\": "src/",
            "SEOService2020\\LaravelTimezone\\Database\\Seeds\\": "database/seeds/"

        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SEOService2020\\LaravelTimezone\\LaravelTimezoneServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "scripts": {
        "pre-package-install": []
    }
}